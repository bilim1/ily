{
  "name": "react-native-cryptopro-rutoken",
  "version": "1.0.0",
  "description": "React Native library for CryptoPro CSP SDK and Rutoken",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace RNCryptoProRutoken.xcworkspace -scheme RNCryptoProRutoken -configuration Release",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.73.0",
    "typescript": "^5.3.0",
    "eslint": "^8.50.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  },
  "keywords": [
    "react-native",
    "cryptopro",
    "csp",
    "rutoken",
    "digital-signature",
    "cryptography"
  ],
  "author": "",
  "license": "MIT"
}
